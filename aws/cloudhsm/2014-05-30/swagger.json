          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/RemoveTagsFromResourceResponse"
            }
          },
          "480": {
            "description": "CloudHsmServiceException",
            "schema": {
              "$ref": "#/definitions/CloudHsmServiceException"
            }
          },
          "481": {
            "description": "CloudHsmInternalException",
            "schema": {
              "$ref": "#/definitions/CloudHsmInternalException"
            }
          },
          "482": {
            "description": "InvalidRequestException",
            "schema": {
              "$ref": "#/definitions/InvalidRequestException"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RemoveTagsFromResourceRequest"
            }
          }
        ]
      },
