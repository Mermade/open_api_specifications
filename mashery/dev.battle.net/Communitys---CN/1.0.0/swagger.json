          {
            "name": "name",
            "type": "string",
            "description": "The name of the profile to retrieve.",
            "in": "path",
            "required": true
          },
          {
            "name": "locale",
            "type": "string",
            "description": "What locale to use in the response",
            "in": "query",
            "required": false,
            "enum": [
              "zh_CN"
            ]
          },
          {
            "name": "callback",
            "type": "string",
            "description": "Request data to be returned as a JsonP callback",
            "in": "query",
